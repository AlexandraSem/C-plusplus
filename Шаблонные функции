#include "String.h"
#include <iostream>
#include <memory>


template<class T>
void BubbleSort(T* arr, int count)
{
	for (int i = count - 1; i > 0; --i)
	{
		for (int j = 0; j < i; ++j)
		{
			if (arr[j] > arr[j + 1])
			{
				// swap(arr[j], arr[j + 1]);
				T tmp = arr[j];
				arr[j] = arr[j + 1];
				arr[j + 1] = tmp;
			}
		}
	}
}

int main()
{
	double arr1[] = { 4.3, 2.5, 45.3, 30.1, 4.2 };
	int arr2[] = { 4, 1, 10, 7, 8 };
	String arr3[] = { "dfgdf", "adasd", "ghgh", "bbf", "cdfd" };

	BubbleSort(arr2, 5);
	for (int i = 0; i < 5; ++i)
	{
		std::cout << arr2[i] << " ";
	}
	std::cout << "\n";

	return 0;
}
